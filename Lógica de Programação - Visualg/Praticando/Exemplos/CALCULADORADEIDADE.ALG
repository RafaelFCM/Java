algoritmo "CalculadoraDeIdade"
var
   Nascimento: real
   Ano: real
   Idade: real
inicio
      Escreva ("Em que ano nós estamos? ")
      Leia (Ano)
      Escreva ("Em que ano você nasceu? ")
      Leia (Nascimento)
      Idade <- (Ano-Nascimento)
      Escreva ("Você tem: ", Idade , "anos")
fimalgoritmo