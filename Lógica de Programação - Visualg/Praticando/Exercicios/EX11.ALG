algoritmo "semnome"
var
   a, b, c: inteiro
   d: real
inicio
      Escreva ("Valor de a: ")
      Leia (a)
      Escreva ("Valor de b: ")
      Leia (b)
      Escreva ("Valor de c: ")
      Leia (c)
      Escreval (a, "x^2 +", b, "x +", c)
      d <- b^2 - 4 * a * c
      Escreva ("Delta =", d)
fimalgoritmo