algoritmo "semnome"
var
a, c: inteiro

procedimento gerador (aP: caractere)
inicio
      escreval ("+-------=======------+")
      para c <- 1 ate a faca
           escreval (aP)
      fimpara
      escreval ("+-------=======------+")
fimprocedimento

inicio
      escreva ("quantas vezes quer reproduzir o procedimento? ")
      leia (a)
      gerador ("aprendendo Portugol")

fimalgoritmo